<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#00EDEDED"
        android:paddingTop="?attr/actionBarSize">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="393dp"
            android:layout_height="100dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:background="#ede387" />

        <ImageButton
            android:id="@+id/B1"
            android:layout_width="98.25dp"
            android:layout_height="98.25dp"
            android:background="#00FFFFFF"
            app:srcCompat="@drawable/ic_dianpu_foreground"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="617dp" />

        <ImageButton
            android:id="@+id/B2"
            android:layout_width="98.25dp"
            android:layout_height="98.25dp"
            android:background="#00FFFFFF"
            app:srcCompat="@drawable/ic_dingdan_background"
            tools:layout_editor_absoluteX="98dp"
            tools:layout_editor_absoluteY="617dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginBottom="5dp"
            android:text="搜索拼单"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/B2" />

        <ImageButton
            android:id="@+id/B3"
            android:layout_width="98.25dp"
            android:layout_height="98.25dp"
            android:background="#00FFFFFF"
            app:srcCompat="@drawable/ic_wode_background"
            tools:layout_editor_absoluteX="196dp"
            tools:layout_editor_absoluteY="617dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="30dp"
            android:layout_marginBottom="5dp"
            android:text="我的"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/B3" />

        <ImageButton
            android:id="@+id/B4"
            android:layout_width="98.25dp"
            android:layout_height="98.25dp"
            android:background="#00FFFFFF"
            app:srcCompat="@drawable/ic_gengduo_background"
            tools:layout_editor_absoluteX="294dp"
            tools:layout_editor_absoluteY="617dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginBottom="5dp"
            android:text="其他功能"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/B4" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginBottom="5dp"
            android:text="搜索商店"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="393dp"
            android:layout_height="61dp"
            app:layout_constraintTop_toTopOf="parent"
            android:background="#EDE387"
            tools:layout_editor_absoluteX="0dp" />

        <EditText
            android:id="@+id/ET1"
            android:layout_width="310dp"
            android:layout_height="41dp"
            android:layout_marginStart="3dp"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="请输入您要搜索的关键词"
            app:layout_constraintBottom_toBottomOf="@+id/imageView5"
            app:layout_constraintStart_toStartOf="@+id/imageView5"
            app:layout_constraintTop_toTopOf="@+id/imageView5" />

        <ImageButton
            android:id="@+id/Bsearch"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:background="#00FFFFFF"
            app:layout_constraintTop_toTopOf="@+id/imageView5"
            app:srcCompat="@android:drawable/ic_menu_search"
            tools:layout_editor_absoluteX="322dp" />

        <ListView
            android:layout_width="391dp"
            android:layout_height="554dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView4"
            app:layout_constraintTop_toBottomOf="@+id/imageView5"
            tools:layout_editor_absoluteX="0dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>